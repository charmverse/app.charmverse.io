{
  "extends": "./tsconfig.json",
  "exclude": [
    "node_modules",
    "__e2e__"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "paths": {
      "@charmverse/core/prisma": [
        "node_modules/@charmverse/core/dist/esm/prisma"
      ],
      "@charmverse/core/prisma-client": [
        "node_modules/@charmverse/core/dist/esm/prisma-client"
      ],
      "@charmverse/core/shared": [
        "node_modules/@charmverse/core/dist/esm/shared"
      ],
      "@charmverse/core/log": [
        "node_modules/@charmverse/core/dist/esm/lib/log"
      ],
      // "@skiff-org/prosemirror-tables": [
      //   "node_modules/@skiff-org/prosemirror-tables/dist/index.es.js"
      // ],
      "prosemirror-model": [
        "node_modules/prosemirror-model/dist/index.js"
      ],
      "prosemirror-view": [
        "node_modules/prosemirror-view/dist/index.js"
      ],
      "prosemirror-state": [
        "node_modules/prosemirror-state/dist/index.js"
      ],
      "prosemirror-transform": [
        "node_modules/prosemirror-transform/dist/index.js"
      ],
      "prosemirror-utils": [
        "node_modules/prosemirror-utils/dist/index.js"
      ]
    }
  }
}