{
  "scripts": {
    "build": "NODE_OPTIONS=\"--max_old_space_size=4096\" next build",
    "build:win": "npx next build",
    "build:prisma": "prisma generate",
    "build:analyze": "cross-env ANALYZE=true next build",
    "deploy:staging": "npx cdk deploy \"stg-charmverse-${STAGE}\" --outputs-file cdk.out.json",
    "postdeploy:staging": "node --eval \"console.log('::set-output name=env_url::' + require('./cdk.out.json')['stg-charmverse-${STAGE}'].DeploymentUrl)\"",
    "destroy:staging": "npx cdk destroy \"stg-charmverse-${STAGE}\" --force",
    "lint": "eslint -c .eslintrc.json . --ext .ts --ext .tsx --quiet",
    "lint:prod": "npm run lint -- --quiet",
    "cron:staging": "npx react-env --path .env.staging -- node ./.next/server/cron.js",
    "cron:prod": "node ./.next/server/cron.js",
    "cron:dev": "dotenv -e .env.local -- ts-node ./background/cron.ts",
    "migrate:test": "dotenv -e .env.test.local -- npx prisma migrate deploy",
    "migrate:prod": "prisma migrate deploy",
    "migrate:staging": "npx dotenv -e .env.staging -- prisma migrate deploy",
    "server:dev": "next dev",
    "debug:e2e": "dotenv -e .env.test.local -- npx playwright test --headed --timeout=0",
    "notifications:debug": "ts-node ./scripts/readNotifications.ts",
    "server:prod": "node --require dd-trace/init node_modules/.bin/next start",
    "server:prod:win": "node --require dd-trace/init node_modules/next/dist/bin/next start",
    "server:staging": "npx react-env --path .env.staging -- node --require dd-trace/init node_modules/.bin/next start",
    "start": "npm run server:dev",
    "start:debug": "NODE_OPTIONS='--inspect' next",
    "start:dev:cron": "concurrently --names \"cron,next\" -c \"yellow,green\" \"npm run cron:dev\" \"npm run server:dev\"",
    "postinstall": "patch-package",
    "start:test": "NODE_OPTIONS=\"--max_old_space_size=4096\" dotenv -e .env.test.local -- prisma migrate deploy && dotenv -e .env.test.local -- npm start",
    "start:test:ci": "NODE_OPTIONS=\"--max_old_space_size=4096\" npx react-env --path .env.test.local -- npx next start",
    "start:prod": "npm run migrate:prod && npm run server:prod",
    "start:staging": "npm run migrate:staging && npm run server:staging",
    "start:remote": "cross-env NODE_ENV=development npx react-env --path .env.production -- next dev",
    "prepare": "husky install",
    "db-tool": "npx prisma studio",
    "db-tool:dev": "dotenv -e .env.local -- npx prisma studio",
    "db-tool:test": "dotenv -e .env.test.local -- npx prisma studio",
    "test:typecheck": "tsc --project tsconfig.test.json --noEmit",
    "test:setup-db": "./scripts/configure-db.sh",
    "test:browser": "npm run test-setup && jest --config='./jest.config-client.ts'",
    "test:e2e": "dotenv -e .env.test.local -- npx playwright test --timeout=0",
    "test": "npm run test:ci -- --verbose --watch",
    "test:ci": "NODE_OPTIONS=\"--max_old_space_size=4096\" dotenv -e .env.test.local --override -- npx jest --config='./jest.config.ts' --maxWorkers=2",
    "test:debug": "dotenv -e .env.test.local --override -- node --inspect-brk --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage --config='./jest.config-server.ts' --verbose",
    "test:server": "npm run test:server:ci -- --verbose --watch",
    "test:server:ci": "NODE_OPTIONS=\"--max_old_space_size=4096\" dotenv -e .env.test.local --override -- npx jest --config='./jest.config-server.ts' --maxWorkers=2",
    "test:browser:ci": "dotenv -e .env.test.local --override -- npx jest --config='./jest.config-browser.ts'",
    "test:e2e:ci": "NODE_OPTIONS=\"--max_old_space_size=4096\" dotenv -e .env.test.local -- npx playwright test"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
