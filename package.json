{
  "scripts": {
    "build": "next build",
    "build:prod": "prisma generate && npm run build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "deploy:staging": "npx cdk deploy \"CharmVerse-staging-${STAGE}\" --outputs-file cdk.out.json",
    "postdeploy:staging": "node --eval \"console.log('::set-output name=env_url::' + require('./cdk.out.json')['CharmVerse-staging-${STAGE}'].DeploymentUrl)\"",
    "destroy:staging": "npx cdk destroy \"CharmVerse-staging-${STAGE}\" --force",
    "lint": "eslint -c .eslintrc.json . --ext .ts --ext .tsx",
    "lint:prod": "npm run lint -- --quiet",
    "start": "next dev",
    "start:prod": "prisma migrate deploy && npx next start",
    "start:staging": "npx dotenv -e .env.staging -- prisma migrate deploy && npx react-env --path .env.staging -- npx next start",
    "start:remote": "npx react-env --path .env.production -- next dev",
    "prepare": "husky install",
    "db-tool": "dotenv -e .env.production -- npx prisma studio",
    "db-tool:dev": "dotenv -e .env.local -- npx prisma studio"
  },
  "postinstall": "patch-package",
  "prepare": "husky install",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
