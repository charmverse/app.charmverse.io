{
  "name": "webapp",
  "version": "0..0",
  "private": true,
  "scripts": {
    "start": "npx dotenv -e .env.local -- npx next dev",
    "build": "npx next build",
    "start:prod": "npx next start",
    "lint": "npx next lint",
    "typecheck": "../../node_modules/typescript/bin/tsc --project ./tsconfig.json  --noEmit",
    "start:test:ci": "NODE_OPTIONS=\"--max_old_space_size=4096\" npx react-env --path .env.test.local -- npx next start",
    "test:ci": "node --experimental-vm-modules ../../node_modules/.bin/jest --maxWorkers=2 --passWithNoTests",
    "test:integration": "",
    "test:browser": "npx vitest --globals --update --project=browser",
    "test:lib": "npx vitest --globals --update --project=lib",
    "test:e2e": "NODE_OPTIONS=--experimental-require-module npx playwright test --timeout=0",
    "test:e2e:ci": "NODE_OPTIONS=\"--max_old_space_size=4096 --experimental-require-module\" npx playwright test",
    "debug:e2e": "NODE_OPTIONS=--experimental-require-module npx playwright test --headed --timeout=0"
  },
