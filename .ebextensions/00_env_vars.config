option_settings:
    aws:elasticbeanstalk:application:environment:
        AUTH_SECRET: '{{resolve:ssm:/io.cv.app/staging/auth_secret}}'
        DB_HOSTNAME:  '{{resolve:ssm:/io.cv.app/staging/db/host}}'
        DB_NAME:  '{{resolve:ssm:/io.cv.app/staging/db/dbname}}'
        DB_PASSWD: '{{resolve:ssm:/io.cv.app/staging/db/password}}'
        DB_USER: '{{resolve:ssm:/io.cv.app/staging/db/username}}'
        DD_API_KEY: '{{resolve:ssm:/io.cv.app/shared/datadog/api_key}}'
        DEEP_DAO_BASE_URL: "https://api.deepdao.io"
        NEXT_PUBLIC_TELEGRAM_BOT_ID: '{{resolve:ssm:/io.cv.app/staging/public_telegram_bot_id}}'
        SERVICE_ENV: "stg"           # this sets the value of datadog env tag
        SERVICE_NAME: "webapp"
        NODE_ENV: "staging"
        NPM_SCRIPT: "start:staging"
        COMPOSE_PROJECT_NAME: '`{"Ref" : "AWS::ElasticBeanstalk::Environment"}`'